proc b1(val int b)
begin
    write b;
    write "\n";
end

proc b2()
    int b;
begin
    b := 3;
    write b;
    write "\n";
end

proc main_helper(val int a)
    int b;
begin
    # from formal params
    call b1(a);
    # from decl
    b := 1;
    call b1(b);
    # direct value
    call b1(2);
end

